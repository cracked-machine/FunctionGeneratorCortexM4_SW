#ifndef SRC_SIGNALMANAGER_FUNCTIONOUTPUT_H_
#define SRC_SIGNALMANAGER_FUNCTIONOUTPUT_H_


typedef enum
{
	Sine_Out_Mode,
	Square_Out_Mode,
	Saw_Out_Mode,
	RevSaw_Out_Mode,
	Triangle_Out_Mode,
	Impulse_Out_Mode

} eOutput_mode;

/*
 *	Array of objects for Function Presets and their encoder positions for func preset menu
 */
#define MAX_NUM_FPRESETS 6

typedef struct
{
	eOutput_mode hertz;
	uint8_t epos;

} Freq_Preset_Encoder_Pos_t;

Freq_Preset_Encoder_Pos_t aFreqPresetEncoderPos[MAX_NUM_FPRESETS];

/*
 * 		The Currently Used Frequency Preset
 */
Freq_Preset_Encoder_Pos_t *pNewFreqPresetEncoderPos;

void FuncO_ModifyOutput();

eOutput_mode FuncO_GetOutputMode();


#endif /* SRC_SIGNALMANAGER_FUNCTIONOUTPUT_H_ */
