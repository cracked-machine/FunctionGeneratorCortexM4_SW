#include "DacTimerRegistry.h"

/*
 *
 */
DacTimeReg_t DacTimerReg[MAX_DAC_TIMER_SETTINGS] =
{

		{0, FPRESET_1HZ, 	0, 0},			// 1Hz
		{1, FPRESET_10HZ, 	0, 0},			// 10Hz
		{2, FPRESET_50HZ,	0, 0},			//
		{3, FPRESET_100HZ,	0, 0},
		{4, FPRESET_250HZ, 	0, 0},
		{5, FPRESET_500HZ,	0, 0},
		{6, FPRESET_750HZ,	0, 0},
		{7, FPRESET_1KHZ,	0, 0},
		{8, FPRESET_5KHZ,	0, 0},
		{9, FPRESET_10KHZ,	0, 0},
		{10,FPRESET_25KHZ,  0, 0},
		{11,FPRESET_50KHZ,  0, 0},
		{12,FPRESET_75KHZ,  0, 0},
		{13,FPRESET_100KHZ, 0, 0}

};

/*
 *
 *
 *
 */
void DT_InitRegister()
{
	for(int i = 0; i < MAX_DAC_TIMER_SETTINGS; i++)
	{

		DacTimerReg[i].arr;
	}
}

