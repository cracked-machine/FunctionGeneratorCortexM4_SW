#ifndef SRC_SIGNALMANAGER_DACTIMER_H_
#define SRC_SIGNALMANAGER_DACTIMER_H_

#include <stdint.h>

#define MAX_DAC_TIMER_SETTINGS 14

typedef struct
{
	uint8_t 	id;
	uint16_t	psc;
	uint16_t	arr;

} DacTimeReg_t;

DacTimeReg_t DacTimerReg[MAX_DAC_TIMER_SETTINGS];

void DT_InitRegister();
DacTimeReg_t* DT_GetRegister(uint8_t pIndex);

#endif /* SRC_SIGNALMANAGER_DACTIMER_H_ */
