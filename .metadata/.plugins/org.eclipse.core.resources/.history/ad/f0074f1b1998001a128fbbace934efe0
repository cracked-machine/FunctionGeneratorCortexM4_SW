#ifndef SRC_EVENTMANAGER_OUTPUTMENUSTATEHANDLER_H_
#define SRC_EVENTMANAGER_OUTPUTMENUSTATEHANDLER_H_

#include "EventManager.h"

/*
 * 	Sub menus should have their ExitHandlers set eXXXXMenu_Status to ENABLE_xxxx_MAIN_MENU
 */
typedef enum
{
	DISABLE_OUTPUT_MENU = 0x00U,
	ENABLE_OUTPUT_MAIN_MENU,
	ENABLE_OUTPUT_FUNC_MENU,
	ENABLE_OUTPUT_FREQ_MENU,
	ENABLE_OUTPUT_GAIN_MENU,
	ENABLE_OUTPUT_BIAS_MENU

} eOutputMenu_Status;







eOutputMenu_Status OutputMenu_getStatus();

eSystemState OutputMainMenuEntryHandler();
eSystemState OutputMainMenuInputHandler();
eSystemState OutputMainMenuExitHandler();



#endif /* SRC_EVENTMANAGER_OUTPUTMENUSTATEHANDLER_H_ */
