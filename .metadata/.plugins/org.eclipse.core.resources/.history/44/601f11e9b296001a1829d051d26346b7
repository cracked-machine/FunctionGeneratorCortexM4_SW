#ifndef SRC_SIGNALMANAGER_DACCHANNEL_H_
#define SRC_SIGNALMANAGER_DACCHANNEL_H_


#include "FunctionOutput.h"
#include "dac.h"

typedef struct
{
	DAC_HandleTypeDef *dac;
	uint32_t channel;
