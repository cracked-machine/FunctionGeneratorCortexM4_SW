void IM_BTN4_EXTI1_Handler()
{
	if (LL_EXTI_IsActiveFlag_0_31(LL_EXTI_LINE_1))
	{
		EM_SetNewEvent(evGreenBtn);
		printf("'Green' BTN4_EXTI1_Pin\n");
	}
}

